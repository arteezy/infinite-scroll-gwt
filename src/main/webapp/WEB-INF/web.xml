<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <servlet>
        <servlet-name>com.hiringtask.TableOfDudes TableOfDudesService</servlet-name>
        <servlet-class>com.hiringtask.server.TableOfDudesServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.hiringtask.TableOfDudes TableOfDudesService</servlet-name>
        <url-pattern>/TableOfDudes/TableOfDudesService</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>gwtRequest</servlet-name>
        <servlet-class>com.google.web.bindery.requestfactory.server.RequestFactoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gwtRequest</servlet-name>
        <url-pattern>/gwtRequest</url-pattern>
    </servlet-mapping>
</web-app>
